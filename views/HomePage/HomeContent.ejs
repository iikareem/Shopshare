<section class="shop_section layout_padding">
    <div class="container">
        <div class="heading_container heading_center">
            <h2>
                Latest Products
            </h2>
        </div>
        <div class="row">
            <div class="col-md-6 ">
                <div class="box">
                    <a href="">
                        <div class="img-box">
                            <img src="home/images/w1.png" alt="">
                        </div>
                        <div class="detail-box">
                            <h6>
                                Smartwatch
                            </h6>
                            <h6>
                                Price:
                                <span>
                    $300
                  </span>
                            </h6>
                        </div>
                        <div class="new">
                <span>
                  Featured
                </span>
                        </div>
                    </a>
                </div>
            </div>


            <% if (Products.length > 0) { for(var i=ProductsNumber-1; i >= ProductsNumber-6; i--) { %>
            <div class="col-sm-6 col-xl-3">
                <div class="box">
                    <a href="/catalog/Detail/<%=Products[i]._id%>">
                        <div class="img-box">
                            <img src="<%=Products[i].IMAGE_LABEL%>>" alt="">
                        </div>
                        <div class="detail-box">
                            <h6>
                                <%=Products[i].Ad_Title%>
                            </h6>
                            <h6>
                                Price:
                                <span>
                    <%=Products[i].Product_Price%>
                  </span>
                            </h6>
                        </div>
                        <div class="new">
                <span>
                  New
                </span>
                        </div>
                    </a>
                </div>
            </div>
            <%  }}   %>

        </div>

        <div class="btn-box">
            <a href="/catalog">
                View All
            </a>
        </div>
    </div>
</section>



